public class Main
{
    static int squareRootOfANumber(int num)
    {
        int low = 1 , high = num;
        while(low <= high)
        {
            int mid = low +(high-low)/2;
            if(mid * mid == num)
            {
                return mid;
            }
            if(mid*mid > num) high = mid-1;
            else low = mid+1;
        }
        return high;
    }
	public static void main(String[] args) {
	    int num = 34;
		int squareRoot = squareRootOfANumber(num);
		System.out.println(squareRoot);
	}
}
