class HelloWorld {
    private static int findingThePowerUsingRecurion(int a,int b)
    {
        if(b == 0) return 1;
        return a*findingThePowerUsingRecurion(a,b-1);
    }
    public static void main(String[] args) {
        java.util.Scanner sc = new java.util.Scanner(System.in);
        // System.out.println("Enter your number");
        System.out.println("Fibonnaci number at given number is "+findingThePowerUsingRecurion(sc.nextInt(),sc.nextInt()));
    }
}
